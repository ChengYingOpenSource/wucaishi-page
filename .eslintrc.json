{
  "extends": "eslint-config-ali/react",
  "env": {
    "browser": true,
    "node": true
    },
  "rules": {
    "semi": [1, "always"],
    "max-len": 1,
    "react/destructuring-assignment": 0,
    "react/prop-types": 0,
    "no-nested-ternary": 0,
    "no-return-assign": 0,
    "no-param-reassign": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "legacyDecorators": true
    }
  },
  "settings": {
    "import/resolver": "node"
  }
}
